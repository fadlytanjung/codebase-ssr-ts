{
  "name": "codebase-ssr-ts",
  "version": "1.0.0",
  "description": "Codebase SSR with typescript and nextjs",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fadlytanjung/codebase-ssr-ts.git"
  },
  "scripts": {
    "build": "next build",
    "build:staging": "env-cmd -f .env.test next build",
    "build:production": "env-cmd -f .env.production next build",
    "dev": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "lint": "esw src/pages/ src/components/ src/actions/ --color --ext .ts,.tsx,.js",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "next dev",
    "precommit": "npm run test && npm run lint",
    "start": "next start",
    "start:staging": "env-cmd -f .env.test next start",
    "start:production": "env-cmd -f .env.production next start",
    "test": "jest test --passWithNoTests",
    "test:cover": "npm run test -- --coverage",
    "test:ci": "jest --ci",
    "prepare": "husky install"
  },
  "author": "Fadly Tanjung",
  "license": "ISC",
  "dependencies": {
    "@emotion/cache": "^11.10.2",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.96",
    "@mui/material": "^5.10.1",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "next": "^12.2.5",
    "next-auth": "^4.10.3",
    "next-redux-wrapper": "^7.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@emotion/babel-plugin": "^11.10.0",
    "@next/eslint-plugin-next": "^12.2.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.9",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "postcss": "^8.4.16",
    "sass": "^1.54.5",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  }
}
